    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^25.2.2",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "gulp-watch": "^5.0.1",
    "husky": "^4.2.5",
    "hygen": "^6.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^10.2.0",
