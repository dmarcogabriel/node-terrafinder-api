{
  "name": "node-express",
  "version": "0.0.4",
  "description": "This is a API for Terrafinder app",
  "main": "dist/index.js",
  "engines": {
    "node": ">= 14.x",
    "yarn": ">= 1.22.5"
  },
  "scripts": {
    "#": "============== RUN ==============",
    "start": "node build",
    "dev": "nodemon src/index.ts",
    "start:stg": "echo 'Not working yet'",
    "start:prod": "yarn build && node dist",
    "##": "============== BUILD ==============",
    "build:prod": "rimraf dist && tsc",
    "###": "============== TESTING ==============",
    "test": "jest --watchAll",
    "test:cov": "jest --coverage",
    "####": "============== LINTING ==============",
    "lint": "eslint src --ext .ts",
    "#####": "============== OTHER ==============",
    "gen": "hygen"
  },
  "author": "Gabriel De Marco",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "body-parser": "1.19.0",
    "chalk": "^4.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.25.0",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-fileupload": "^1.2.0",
    "express-validator": "^6.13.0",
    "fs-extra": "^10.0.0",
    "helmet": "^4.2.0",
    "inquirer": "^8.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.3",
    "nodemailer": "^6.7.0"
