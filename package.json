{
  "name": "node-express",
  "version": "1.0.0",
  "description": "This is a API for Terrafinder app",
  "main": "src/index.js",
  "engines": {
    "node": ">= 14.x",
    "yarn": ">= 1.22.5"
  },
  "scripts": {
    "dev": "nodemon",
    "start": "node src/index.js",
    "test": "jest --watchAll",
    "test:cov": "jest --coverage",
    "lint": "eslint ./src --fix",
    "lint:watch": "esw --quiet -w",
    "generate": "hygen"
  },
  "keywords": [
    "express",
    "node",
    "nodemon",
    "bcrypt",
    "body-parser",
    "compression",
    "cors",
    "dotenv",
    "helmet",
    "jsonwebtoken",
    "mongoose",
    "heroku"
  ],
  "author": "Gabriel De Marco",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint-watch": "^6.0.1",
    "express": "4.17.1",
    "express-fileupload": "^1.2.0",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.2.5",
    "hygen": "^6.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^10.2.0",
    "mock-jwks": "^0.3.1",
    "mongodb-memory-server": "^6.9.2",
    "nock": "^13.0.5",
    "nodemon": "2.0.2",
    "prettier": "^2.0.2",
    "supertest": "^6.0.1",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint src --fix",
      "jest --bail"
    ]
  }
}
